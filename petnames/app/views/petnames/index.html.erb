<p id="notice"><%= notice %></p>

<h1>Listing Petnames</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @petnames.each do |petname| %>
      <tr>
        <td><%= petname.name %></td>
        <td><%= link_to 'Show', petname %></td>
        <td><%= link_to 'Edit', edit_petname_path(petname) %></td>
        <td><%= link_to 'Destroy', petname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Petname', new_petname_path %>
